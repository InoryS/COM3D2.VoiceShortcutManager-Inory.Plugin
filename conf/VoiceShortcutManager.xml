<?xml version="1.0" encoding="utf-8"?>

<Config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <version>2.1</version>
    <micActivateOnStart>false</micActivateOnStart>
    <micEnabled>true</micEnabled>
    <micLabelON>音声認識 ON</micLabelON>
    <micLabelOFF>音声認識 OFF</micLabelOFF>
    <keyboardEnabled>true</keyboardEnabled>
    <keyboardLabelON>テンキー表示 ON</keyboardLabelON>
    <keyboardLabelOFF>テンキー表示</keyboardLabelOFF>
    <keybordOffsetY>60</keybordOffsetY>
    <buttonSize>32</buttonSize>
    <fontSize>16</fontSize>
    <color>
        <r>96</r>
        <g>96</g>
        <b>96</b>
        <a>240</a>
    </color>
    <forceVoiceYotogiCommand>false</forceVoiceYotogiCommand>
    <keepExplanationYotogi>false</keepExplanationYotogi>
    <keepExplanation>false</keepExplanation>
    <useUndressManager>true</useUndressManager>
    <playVoicceMaidPos>true</playVoicceMaidPos>
    <vymEnabled>true</vymEnabled>
    <vymCamMoveEnabled>true</vymCamMoveEnabled>
    <vymUnzipEnabled>true</vymUnzipEnabled>
    <vymFaceMotionEnabled>true</vymFaceMotionEnabled>
    <vymUndressLinkType>1</vymUndressLinkType>
    <vymEyeFaceLinkType>1</vymEyeFaceLinkType>
    <vymVibeLinkType>1</vymVibeLinkType>
    <vymFaceLinkType>0</vymFaceLinkType>
    <vymMotionLinkType>0</vymMotionLinkType>
    <faceFadeTime>0.5</faceFadeTime>
    <motionFadeTime>0.7</motionFadeTime>
    <menuList>
        <MenuInfo>
            <name>PropMyItem</name>
            <label>PropMyItem</label>
            <key>I</key>
            <shift>true</shift>
            <ctrl>false</ctrl>
            <alt>false</alt>
        </MenuInfo>
        <MenuInfo>
            <name>ModsSlider</name>
            <label>ModsSlider</label>
            <key>F5</key>
            <shift>false</shift>
            <ctrl>false</ctrl>
            <alt>false</alt>
            <sceneRegex>^SceneEdit$</sceneRegex>
        </MenuInfo>
        <MenuInfo>
            <name>YotogiSlider</name>
            <label>YotogiSlider</label>
            <key>F5</key>
            <shift>false</shift>
            <ctrl>false</ctrl>
            <alt>false</alt>
            <sceneRegex>^SceneYotogi$</sceneRegex>
        </MenuInfo>
        <MenuInfo>
            <name>Pause</name>
            <label>Pause</label>
            <voice>
                <string>ポーズ</string>
                <string>停止</string>
            </voice>
            <key>Enter</key>
            <shift>false</shift>
            <ctrl>false</ctrl>
            <alt>false</alt>
            <sceneRegex>^(SceneDance_|SceneVR)</sceneRegex>
        </MenuInfo>
    </menuList>
</Config>